{
  "mcpServers": {
    "deepwiki": {
      "serverUrl": "https://mcp.deepwiki.com/sse"
    },
    "Sentry": {
      "url": "https://mcp.sentry.dev/sse"
    },
    "GitHub": {
      "command": "docker run -i --rm -e GITHUB_PERSONAL_ACCESS_TOKEN ghcr.io/github/github-mcp-server",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "Figma": {
      "url": "http://127.0.0.1:3845/sse"
    },
    "Playwright": {
      "command": "npx -y @modelcontextprotocol/server-playwright",
      "env": {}
    },
    "context7": {
      "url": "https://mcp.context7.com/mcp"
    },
    "agents-playbook": {
      "url": "https://agents-playbook.vercel.app/api/mcp"
    },
    "Supabase": {
      "command": "npx -y @supabase/mcp-server-postgrest@latest --apiUrl ${SUPABASE_API_URL} --apiKey ${SUPABASE_ANON_KEY} --schema public",
      "env": {}
    },
    "fohlio": {
      "url": "http://fohlio-mcp.vercel.app/api/mcp",
      "env": {
        "FOHLIO_TOKEN": "${FOHLIO_TOKEN}"
      }
    },
    "browser-tools": {
      "command": "npx @agentdeskai/browser-tools-mcp@latest"
    },
    "Vercel": {
      "url": "https://mcp.vercel.com",
      "headers": {}
    }
  }
}